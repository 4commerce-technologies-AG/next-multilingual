{
  "name": "next-multilingual-alternate",
  "description": "An opinionated end-to-end solution for Next.js applications that requires multiple languages.",
  "version": "0.10.3",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./config": "./dist/config/index.js",
    "./link": "./dist/link/index.js",
    "./link/ssr": "./dist/link/ssr.js",
    "./head": "./dist/head/index.js",
    "./head/ssr": "./dist/head/ssr.js",
    "./messages": "./dist/messages/index.js",
    "./messages/babel-plugin": "./dist/messages/babel-plugin.js"
  },
  "typesVersions": {
    "*": {
      "config": [
        "dist/config/index.d.ts"
      ],
      "link": [
        "dist/link/index.d.ts",
        "dist/link/ssr.d.ts"
      ],
      "head": [
        "dist/head/index.d.ts",
        "dist/head/ssr.d.ts"
      ],
      "messages": [
        "dist/messages/index.d.ts",
        "dist/messages/babel-plugin.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "Next",
    "Next js",
    "Next.js",
    "multilingual",
    "i18n",
    "internationalization",
    "intl",
    "localization",
    "l10n",
    "globalization",
    "g11n",
    "ICU",
    "MessageFormat"
  ],
  "author": "Tom Freudenberg, 4commerce technologies AG",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4commerce-technologies-AG/next-multilingual-alternate.git"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=14"
  },
  "watch": {
    "build": {
      "patterns": "*",
      "extensions": "*.ts?(x)",
      "ignore": "node_modules"
    }
  },
  "scripts": {
    "build_watch": "npm-watch",
    "build": "rm -Rf ./dist && tsc",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix",
    "next-lint": "cd example && next lint",
    "release": "dotenv -- release-it --only-version",
    "start-example": "cd example && npm run dev",
    "start-example-build": "cd example && npm run build && npm run start",
    "cypress": "cypress open",
    "cypress:build": "cypress open --env isProd=true",
    "cypress:headless": "cypress run",
    "cypress:build:headless": "cypress run --env isProd=true",
    "e2e": "start-server-and-test start-example http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start-example http://localhost:3000 cypress:headless",
    "e2e:build": "start-server-and-test start-example-build http://localhost:3000 cypress:build",
    "e2e:build:headless": "start-server-and-test start-example-build http://localhost:3000 cypress:build:headless"
  },
  "dependencies": {
    "@babel/core": "^7.16.12",
    "cheap-watch": "^1.0.4",
    "colorette": "^2.0.16",
    "dot-properties": "^1.0.1",
    "js-yaml": "^4.1.0",
    "intl-messageformat": "^9.11.3",
    "nookies": "^2.5.2",
    "resolve-accept-language": "^1.0.48"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.1.0",
    "@types/babel__core": "^7.1.18",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@typescript-eslint/types": "^5.10.1",
    "cypress": "^9.3.1",
    "cypress-fail-on-console-error": "^2.1.3",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "v12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "next": "v12.0.9",
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "release-it": "^14.12.4",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bugs": {
    "url": "https://github.com/4commerce-technologies-AG/next-multilingual-alternate/issues"
  },
  "homepage": "https://github.com/4commerce-technologies-AG/next-multilingual-alternate#readme",
  "directories": {
    "doc": "docs",
    "example": "example",
    "dist": "dist"
  }
}
